
package atividadegrafo2;

public class Arestas {
    private int origem;
    private int destino;
    
    public Arestas(int origem, int destino){
        this.origem=origem;
        this.destino=destino;
    }

    public int getOrigem() {
        return origem;
    }

    public void setOrigem(int origem) {
        this.origem = origem;
    }

    public int getDestino() {
        return destino;
    }

    public void setDestino(int destino) {
        this.destino = destino;
    }
    
    
}
